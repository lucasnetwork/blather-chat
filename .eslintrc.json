{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["solid","prettier"],
    "extends": ["eslint:recommended", "plugin:solid/typescript","prettier"],
     "parser": "@typescript-eslint/parser", 
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier":"error"
    }
}
